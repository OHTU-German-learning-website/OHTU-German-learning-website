{
  "name": "gradesa",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.2.4",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "sh ./data/migrate.sh --skip-schema-gen && next start",
    "lint": "next lint",
    "test": "vitest --disable-console-intercept",
    "test:coverage": "vitest --disable-console-intercept --coverage.enabled=true",
    "test:coverage:ui": "vitest --ui --api.port=51204 --api.host=0.0.0.0 --open=false --disable-console-intercept --coverage.enabled=true",
    "test:coverage:ui:localhost": "vitest --ui --api.port=51204 --api.host=127.0.0.1 --open=false --disable-console-intercept --coverage.enabled=true",
    "test:e2e": "NODE_ENV=test playwright test",
    "test:e2e:ui": "NODE_ENV=test playwright test --ui",
    "test:e2e:debug": "NODE_ENV=test playwright test --debug"
  },
  "dependencies": {
    "@faker-js/faker": "^9.4.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.2.0",
    "axios": "^1.7.9",
    "dompurify": "^3.3.0",
    "dotenv": "^16.4.7",
    "immutability-helper": "^3.1.1",
    "jose": "^5.10.0",
    "jsdom": "^26.0.0",
    "mailersend": "^2.3.0",
    "next": "^15.5.6",
    "pg": "^8.13.1",
    "qs": "^6.14.0",
    "quill": "^2.0.3",
    "react": "^19.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-frame-component": "^5.2.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.6",
    "@vitest/ui": "^3.0.6",
    "eslint": "^9",
    "eslint-config-next": "15.1.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-unused-imports": "^4.1.4",
    "happy-dom": "^20.0.7",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "vitest": "^3.0.4"
  }
}
